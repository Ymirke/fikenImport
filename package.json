{
  "name": "fikenimport",
  "version": "0.0.1",
  "description": "Making it easy to import a list of purchases into fiken.",
  "main": "/backend/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "client": "cd client && npm start",
    "server": "cd backend && node index.js",
    "installClient": "cd client && yarn",
    "installBackend": "cd backend && npm install",
    "intallAll": "npm install && npm run installClient && npm run installBackend",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ymirke/fikenImport.git"
  },
  "author": "Ymir Egilson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ymirke/fikenImport/issues"
  },
  "eslintConfig": {
    "extends": [
      "salt",
      "salt/react",
      "salt/react/hooks"
    ]
  },
  "homepage": "https://github.com/Ymirke/fikenImport#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "eslint": "^6.6.0",
    "eslint-config-salt": "^1.0.1"
  }
}